---
# secrets.yml.example - Template for sensitive variables
# Copy to secrets.yml and encrypt with ansible-vault
#
# ansible-vault create vars/secrets.yml
# ansible-vault edit vars/secrets.yml

#==============================================================================
# MIST CLOUD CREDENTIALS
#==============================================================================
mist_org_id: "xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx"
mist_api_token: "your-api-token-here"

#==============================================================================
# SWITCH CREDENTIALS
#==============================================================================
switch_user: admin
switch_password: "CHANGE_ME"

#==============================================================================
# RADIUS CREDENTIALS
#==============================================================================
radius_primary_ip: "10.10.100.50"
radius_secondary_ip: "10.10.100.51"
radius_secret: "CHANGE_ME_MIN_32_CHARS"

#==============================================================================
# WLAN PSKs
#==============================================================================
wlan_psks:
  guest: "GuestPassword123!"
  iot: "IoTSecureKey456!"
