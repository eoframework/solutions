---
# secrets.yml.example - Template for sensitive variables
# Copy to secrets.yml and encrypt with ansible-vault
#
# ansible-vault create vars/secrets.yml
# ansible-vault edit vars/secrets.yml

#==============================================================================
# DEVICE CREDENTIALS
#==============================================================================
junos_user: admin
junos_password: "CHANGE_ME"
junos_root_password_hash: "$6$..."  # Generate with: openssl passwd -6
junos_admin_user: ansible
junos_admin_password_hash: "$6$..."

#==============================================================================
# TACACS+ CREDENTIALS
#==============================================================================
tacacs_server: "10.0.0.50"
tacacs_secret: "CHANGE_ME"

#==============================================================================
# SYSLOG DESTINATION
#==============================================================================
syslog_destination: "10.0.0.100"

#==============================================================================
# VPN CREDENTIALS
#==============================================================================
vpn_psk: "CHANGE_ME_MIN_32_CHARS_REQUIRED_"

#==============================================================================
# ATP CREDENTIALS
#==============================================================================
atp_api_key: "CHANGE_ME"

#==============================================================================
# SECURITY DIRECTOR
#==============================================================================
security_director_password: "CHANGE_ME"
